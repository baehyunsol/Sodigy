# A file that doesn't make the compiler panic!

@test.eq(61)
let main = ackermann(3, 3);

let ackermann(m: Int, n: Int): Int = if m == 0 {
    n + 1
} else if n == 0 {
    ackermann(m - 1, 1)
} else {
    ackermann(m - 1, ackermann(m, n - 1))
};

@test.eq([1, 7, 125])
let ackermann_test = [
    ackermann(0, 0),
    ackermann(2, 2),
    ackermann(3, 4),
];
