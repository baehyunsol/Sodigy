# A file that doesn't make the compiler panic!

def main = ([1, 2, 3, (4 + 5, 4)][3], [1, 1, 0, ][2]);

def ackermann(m: Int, n: Int): Int = if m == 0 {
    n + 1
} else if n == 0 {
    ackermann(m - 1, 1)
} else {
    ackermann(m - 1, ackermann(m, n - 1))
};

@test.eq([1, 7, 125])
def ackermann_test = [
    ackermann(0, 0),
    ackermann(2, 2),
    ackermann(3, 4),
];
