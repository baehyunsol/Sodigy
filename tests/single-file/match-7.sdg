fn tuples(a, b, c, d, e, f) = match ((a, b), (c, d), (e, f)) {
    ((0, x), (0, 1), (_, 1)) => (x, 200),
    ((1, _), (x, 0), (0, 1)) => (x, 800),
    ((1, x), (_, 0), (_, 1)) => (x, 720),
    ((1, _), (_, 1), (1, x)) => (x, 850),
    ((x, 0), (y, 1), (0, 1)) => (x, y),
    ((1, 1), (1, x), (_, y)) => (x, y),
    ((_, 1), (1, 1), (1, x)) => (x, 180),
    ((0, 1), (1, 0), (0, 1)) => (840, 300),
    ((0, 0), (x, _), (_, 0)) => (x, 180),
    ((x, 1), (1, 0), (0, y)) => (x, y),
    ((0, 1), (_, 1), (0, 0)) => (750, 800),
    ((1, 0), (0, x), (1, 0)) => (x, 310),
    ((1, 0), (0, 0), (_, _)) => (620, 860),
    ((1, 1), (1, x), (y, 1)) => (x, y),
    ((1, _), (0, x), (_, y)) => (x, y),
    ((1, 1), (1, x), (y, _)) => (x, y),
    ((_, 0), (x, y), (_, 0)) => (x, y),
    ((1, 1), (_, 0), (1, 0)) => (320, 930),
    ((1, 0), (x, 1), (0, 1)) => (x, 880),
    ((0, 0), (_, 1), (_, _)) => (430, 760),
    (x, (1, 0), (0, _)) => x,
    (x, (0, _), (_, 1)) => x,
    ((_, 1), x, (0, _)) => x,
    ((0, _), (_, 1), (1, 0)) => (460, 180),
    ((0, 0), x, (0, _)) => x,
    ((1, 0), (0, 0), x) => x,
    ((0, _), (1, 0), (0, _)) => (380, 310),
    ((0, _), x, (_, 1)) => x,
    ((1, 0), (_, 1), (0, _)) => (160, 240),
    x => x._1,
};

assert tuples(0, 0, 0, 0, 0, 0) == (0, 180);
assert tuples(0, 0, 0, 2, 0, 1) == (0, 0);
assert tuples(0, 0, 0, 2, 1, 2) == (0, 2);
assert tuples(0, 0, 1, 0, 0, 0) == (1, 180);
assert tuples(0, 0, 1, 0, 0, 2) == (0, 0);
assert tuples(0, 0, 1, 1, 0, 0) == (1, 180);
assert tuples(0, 0, 1, 2, 1, 0) == (1, 180);
assert tuples(0, 0, 2, 2, 2, 0) == (2, 180);
assert tuples(0, 1, 0, 1, 0, 0) == (750, 800);
assert tuples(0, 1, 1, 0, 2, 1) == (1, 0);
assert tuples(0, 1, 2, 0, 0, 1) == (2, 0);
assert tuples(0, 1, 2, 2, 2, 0) == (2, 2);
assert tuples(0, 2, 0, 0, 2, 2) == (0, 0);
assert tuples(0, 2, 0, 1, 1, 1) == (2, 200);
assert tuples(0, 2, 1, 0, 0, 1) == (0, 2);
assert tuples(0, 2, 1, 0, 0, 2) == (0, 2);
assert tuples(0, 2, 1, 0, 1, 0) == (1, 0);
assert tuples(0, 2, 1, 2, 2, 0) == (1, 2);
assert tuples(0, 2, 2, 1, 1, 2) == (2, 1);
assert tuples(1, 0, 0, 0, 1, 0) == (0, 310);
assert tuples(1, 0, 1, 1, 2, 1) == (1, 1);
assert tuples(1, 0, 2, 0, 1, 0) == (2, 0);
assert tuples(1, 0, 2, 0, 2, 0) == (2, 0);
assert tuples(1, 0, 2, 0, 2, 2) == (2, 0);
assert tuples(1, 0, 2, 1, 1, 0) == (0, 850);
assert tuples(1, 0, 2, 2, 2, 1) == (2, 2);
assert tuples(1, 1, 0, 0, 1, 1) == (1, 720);
assert tuples(1, 1, 1, 2, 2, 0) == (2, 0);
assert tuples(1, 1, 2, 2, 2, 2) == (2, 2);
assert tuples(1, 2, 0, 0, 1, 1) == (2, 720);
assert tuples(1, 2, 0, 1, 2, 2) == (1, 2);
assert tuples(1, 2, 0, 2, 1, 1) == (2, 1);
assert tuples(1, 2, 1, 1, 1, 0) == (0, 850);
assert tuples(1, 2, 1, 1, 2, 1) == (1, 1);
assert tuples(1, 2, 2, 1, 0, 1) == (2, 1);
assert tuples(2, 0, 0, 0, 1, 1) == (2, 0);
assert tuples(2, 0, 1, 1, 2, 0) == (1, 1);
assert tuples(2, 0, 1, 2, 1, 0) == (1, 2);
assert tuples(2, 1, 0, 1, 0, 0) == (0, 1);
assert tuples(2, 1, 1, 0, 2, 0) == (1, 0);
assert tuples(2, 1, 2, 1, 2, 0) == (2, 1);
assert tuples(2, 1, 2, 2, 0, 0) == (2, 2);
assert tuples(2, 1, 0, 0, 1, 2) == (0, 0);
assert tuples(2, 1, 1, 0, 0, 1) == (2, 1);
assert tuples(2, 1, 2, 0, 0, 0) == (2, 0);
assert tuples(2, 1, 2, 2, 0, 1) == (2, 2);
assert tuples(2, 2, 0, 0, 1, 2) == (0, 0);
assert tuples(2, 2, 0, 2, 1, 1) == (2, 2);
assert tuples(2, 2, 1, 1, 2, 0) == (1, 1);
assert tuples(2, 2, 2, 0, 2, 0) == (2, 0);
assert tuples(2, 2, 2, 2, 2, 2) == (2, 2);
