fn num3(a, b, c) = match (a, b, c) {
    (0, 0, _) => 1,
    (0, _, _) => 2,
    (_, _, 0) => 3,
    (_, _, _) => 4,
};

assert num3(0, 0, 0) == 1;
assert num3(0, 0, 1) == 1;
assert num3(0, 1, 0) == 2;
assert num3(0, 1, 1) == 2;
assert num3(1, 0, 0) == 3;
assert num3(1, 0, 1) == 4;
assert num3(1, 1, 0) == 3;
assert num3(1, 1, 1) == 4;

