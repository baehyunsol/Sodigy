// It's not a closure, but it'd be difficult for the compiler to know that.
assert fibonacci(9) == 55;
let fibonacci = {
    let ONE = 1;
    let TWO = 2;

    \(x) => if x < TWO { ONE } else { fibonacci(x - 1) + fibonacci(x - 2) }
};

fn main() = fibonacci(9) + closure(20);

assert closure(10)(20) == 41;
fn closure(x) = {
    let y = x + 1;

    \(z) => x + y + z
};
