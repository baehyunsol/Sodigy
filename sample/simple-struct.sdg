struct Person = {
    name: String,
    age: Int,
    is_rustacean: Bool,
};

assert bae.name == "Bae";
assert bae.age == 26;
assert bae.is_rustacean == True;
let bae = Person {
    name: "Bae",
    age: 26,
    is_rustacean: True,
};

assert another_bae == "Bae";
fn another_bae() = {
    let bae = Person {
        name: "Bae",
        age: 26,
        is_rustacean: True,
    };

    bae.name
};
